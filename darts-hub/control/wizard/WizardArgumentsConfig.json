    "darts-extern": {
      "displayName": "External Integration",
      "icon": "🔗",
      "description": "Integration with external services and APIs for extended functionality",
      "primaryArguments": [
        "connection",
        "browser_path",
        "autodarts_user",
        "autodarts_password",
        "autodarts_board_id",
        "extern_platform"
      ],
      "sections": {
        "Essential Setup": {
          "priority": 1,
          "expanded": true,
          "arguments": ["connection", "browser_path", "time_before_exit"]
        },
        "Autodarts Connection": {
          "priority": 2,
          "expanded": true,
          "arguments": ["autodarts_user", "autodarts_password", "autodarts_board_id"]
