        "Checkout": {
          "priority": 4,
          "expanded": true,
          "arguments": ["PCC","PCCYO"]
        }
        "Downloads & Updates": {
          "priority": 5,
          "expanded": false,
          "arguments": ["DL", "DLLA", "DLN", "ROVP"]
